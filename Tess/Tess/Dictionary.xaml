<Window x:Class="Tess.Dictionary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dictionary" Height="500" Width="300">
    <Grid>
        <DataGrid x:Name="dictionaryDataGrid" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="340" Width="272" AutoGenerateColumns="True"/>
        <TextBox x:Name="addEntryTextBox" HorizontalAlignment="Left" Height="35" Margin="10,355,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="185"/>
        <Button Content="Dodaj Wpis" HorizontalAlignment="Left" Margin="200,355,0,0" VerticalAlignment="Top" Width="82" Height="35" Click="Button_Click"/>
        <Button Content="Usun wszystko" HorizontalAlignment="Left" Margin="200,395,0,0" VerticalAlignment="Top" Width="82" Height="35" Click="Button_Click_1"/>

    </Grid>
</Window>
